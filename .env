# Environment Variables for Personal Finance App
# Note: Replace placeholder values (xxxxxxxxx) with actual values for your environment

# DynamoDB Tables
USER_TABLE=personal-finance-app-Users-Dev
ACCOUNT_TABLE=personal-finance-app-Accounts-Dev
TRANSACTION_TABLE=personal-finance-app-Transactions-Dev
BUDGET_TABLE=personal-finance-app-Budgets-Dev
GOAL_TABLE=personal-finance-app-Goals-Dev

# S3 Bucket
EXPORT_BUCKET=personal-finance-app-exports-Dev

# Application Stage
STAGE=Dev

# AWS Region
AWS_REGION=us-east-1

# Logging
LOG_LEVEL=info

# Cognito
USER_POOL_ID=us-east-1_xxxxxxxxx
USER_POOL_CLIENT_ID=xxxxxxxxxxxxxxxxxxxxxxxxxx

# API Gateway
API_GATEWAY_URL=https://xxxxxxxxxx.execute-api.us-east-1.amazonaws.com/Dev/
